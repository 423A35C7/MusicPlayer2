        MENUITEM SEPARATOR
        MENUITEM "退出(&X)",                      ID_MENU_EXIT
    END
    POPUP "播放控制(&P)"
    BEGIN
        MENUITEM "播放/暂停(&P)",                   ID_PLAY_PAUSE
        MENUITEM "停止(&S)",                      ID_STOP
        MENUITEM "上一曲(&V)",                     ID_PREVIOUS
        MENUITEM "下一曲(&N)",                     ID_NEXT
        MENUITEM SEPARATOR
        MENUITEM "快退5秒(&R)",                    ID_REW
        MENUITEM "快进5秒(&F)",                    ID_FF
